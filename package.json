    "dompurify": "^3.1.5",
    "interweave": "^13.1.0",
    "marked": "^13.0.1",
    "next": "14.2.4",
    "prisma": "^5.16.1",
    "react": "^18.3.1",
    "react-dom": "^18",
    "react-pdf": "^9.1.0",
    "react-pdf-tailwind": "^2.3.0",
    "simplemde": "^1.11.2",
    "tailwindcss-scoped-groups": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.14.9",
