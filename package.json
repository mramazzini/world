{
  "name": "dndworld",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run seed && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
