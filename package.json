{
  "name": "dndworld",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run seed:prod && next build",
    "build:dev": "npm run seed && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate": "env-cmd -f .env.local npx prisma migrate dev",
    "migrate:prod": "env-cmd -f .env npx prisma migrate deploy",
    "generate": "env-cmd -f .env npx prisma generate",
    "seed": "env-cmd -f .env.local npm run nuke && env-cmd -f .env.local npx prisma db seed",
    "seed:prod": "env-cmd -f .env npx prisma db seed",
    "nuke": "env-cmd -f .env.local npx tsx scripts/nuke.ts",
    "nuke:prod": "env-cmd -f .env npx tsx scripts/nuke.ts",
    "logs": "vercel logs https://world-ashen.vercel.app/",
    "generate:background": "cd scripts/generateSeeds/Backgrounds && npx tsx backgrounds.ts"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "@react-pdf/renderer": "^3.4.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/react-pdf": "^7.0.0",
    "@uiw/react-md-editor": "^4.0.4",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "dompurify": "^3.1.5",
    "fuse.js": "^7.0.0",
    "interweave": "^13.1.0",
    "jose": "^5.6.3",
    "marked": "^13.0.1",
    "next": "14.2.4",
    "prisma": "^5.16.1",
    "prisma-json-types-generator": "^3.0.4",
    "react": "^18.3.1",
    "react-dom": "^18",
    "react-pdf": "^9.1.0",
    "react-pdf-tailwind": "^2.3.0",
    "simplemde": "^1.11.2",
    "tailwindcss-scoped-groups": "^2.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^20.14.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "daisyui": "^4.12.10",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "postcss": "^8",
    "puppeteer": "^23.0.2",
    "tailwindcss": "^3.4.1"
  }
}
